<%- include("header") -%>

    <section class="sec1">
      <h2>Create a new Queue</h2>

      <form class="queueForm" action="/queue/create" method="POST">
        <div class="form-group">
          <label for="queueName">Queue Name:</label>
          <input type="text" class="form-control" name="queueName" required>
        </div>
        <div class="form-group">
          <label for="maxLimit">Max-Limit:</label>
          <input type="number" class="form-control" name="maxLimit" required>
        </div>
        <button type="submit" class="subBtn">Create</button>
      </form>
    </section>

    <!--Local Scripts-->
    <script type="text/javascript" src="/scripts/index.js"></script>
  </body>
</html>
